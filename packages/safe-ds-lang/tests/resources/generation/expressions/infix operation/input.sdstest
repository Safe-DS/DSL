package tests.generator.infixOperation

@Pure fun f(param: Any?)

@Pure fun g() -> result: Boolean

@Pure fun h() -> result: Int

@Pure fun i() -> result: Int?

pipeline test {
    f(g() or g());
    f(g() and g());

    f(h() == h());
    f(h() != h());
    f(h() === h());
    f(h() !== h());

    f(h() < h());
    f(h() <= h());
    f(h() >= h());
    f(h() > h());

    f(h() + h());
    f(h() - h());
    f(h() * h());
    f(h() / h());

    f(i() ?: i());
}
