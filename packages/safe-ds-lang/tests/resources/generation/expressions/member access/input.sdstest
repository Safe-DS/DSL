@Pure fun g() -> result: Boolean

@Pure fun h() -> (result1: Boolean, result2: Boolean)

class C() {
    attr a: Int
    @PythonName("c") attr b: Int

    @Pure
    @PythonCall("$param.i($this)")
    fun i(param: Any?) -> result: Boolean
