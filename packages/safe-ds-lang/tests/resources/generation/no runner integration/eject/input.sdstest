package tests.generator.eject

@Impure([ImpurityReason.Other]) fun f(param: Any?)

@Pure fun g() -> result: Boolean

@Pure fun i() -> result: Int?

@Pure fun factory() -> instance: C?

class C() {
    attr a: Int
    @PythonName("c") attr b: Int
}

pipeline test {
    f(g() or g());
    f(g() and g());

    f(factory()?.a);
    f(factory()?.b);

    f(i() ?: i());
}
