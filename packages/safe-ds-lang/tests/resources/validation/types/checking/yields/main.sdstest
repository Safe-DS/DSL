package tests.validation.types.checking.yields

segment mySegment1() -> result: Int {
    // $TEST$ no error r"Expected type .* but got .*\."
    yield »result« = 1;
}

segment mySegment2() -> result: Int {
    // $TEST$ error "Expected type 'Int' but got 'literal<"">'."
    yield »result« = "";
}

segment mySegment3() {
    // $TEST$ no error r"Expected type .* but got .*\."
    yield »unresolved« = 1;
}
