package safeds.ml.nn

/**
 * @param outputSize The number of neurons in this layer
 * @param inputSize The number of neurons in the previous layer
 */
class FNNLayer(
	@PythonName("output_size") outputSize: Int,
	@PythonName("input_size") inputSize: Int? = null
) {
	/**
	 * Get the output_size of this layer.
	 */
	@PythonName("output_size") attr outputSize: Int
}
