enum MySimpleEnum

@AnnotationUse enum MyComplexEnum {
    MySimpleVariant
    @AnnotationUse MyComplexVariant
        <@AnnotationUse T1, in T2, out T3>
        (@AnnotationUse a: Int, vararg b: Int = 3) 
        where {
            T2 super Number,
            T3 sub Number
        }
}
