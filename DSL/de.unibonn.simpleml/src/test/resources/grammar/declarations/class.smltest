// no_syntax_error

class MySimpleClass

@AnnotationUse
class MyComplexClass
    <@AnnotationUse T1, in T2, out T3>
    (@AnnotationUse a: Int, vararg b: Int = 3)
    sub SuperClass1, SuperClass2
    where T2 super Number, T3 sub Number
{
    @AnnotationUse static attr myAttribute: Int
    @AnnotationUse class MyClass
    @AnnotationUse enum MyEnum
    @AnnotationUse static fun MyFunction()
}
