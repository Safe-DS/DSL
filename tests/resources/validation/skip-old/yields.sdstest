package tests.validation.typeChecking.yields

step myStep1() -> result: Int {

    // $TEST$ no error "A value of type 'Int' cannot be assigned to a result of type 'Int'."
    yield result = »1«;
}

step myStep2() -> result: Int {

    // $TEST$ error "A value of type 'String' cannot be assigned to a result of type 'Int'."
    yield result = »""«;
}
