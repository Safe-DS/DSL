    // $TEST$ no error r"The parameters? .* must be set here\."
    myBlockLambda»(1, 2, 3, 4)«;
    // $TEST$ error "The parameter 'b' must be set here."
    myBlockLambda»(1, c = 2)«;
    // $TEST$ no error r"The parameters? .* must be set here\."
    myBlockLambda»(b = 1, a = 2)«;


    // $TEST$ error "The parameters 'a', 'b' must be set here."
    myExpressionLambda»()«;
    // $TEST$ error "The parameter 'b' must be set here."
